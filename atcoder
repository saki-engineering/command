#!/bin/sh

function usage {
    cat <<EOF
$(basename ${0}) is a tool for ...

Usage:
    $(basename ${0}) [command] [<options>]

Options:
    --version, -v     print $(basename ${0}) version
    --help, -h        print this
EOF
}

baseurl="https://atcoder.jp/contests/"
endpoint="/submissions?"

open "${baseurl}${1}${endpoint}"